<section class="bg-light pt-5 pb-5">
  <div class="container pb-5">
    <%= simple_form_for(@bookmark, url: list_bookmarks_path) do |f| %>
      <%= f.input :comment %>
        <%= f.collection_select :movie_id, Movie.all, :id, :title, prompt: "select movie" %>
      <%= f.submit %>
    <% end %>

  </div>
</section>